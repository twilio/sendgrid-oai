                    "value": {
                      "result": [
                        {
                          "id": "fae7c985-eb92-4b47-9987-28ec29dbc698",
                          "name": "example_name",
                          "generation": "legacy",
                          "updated_at": "2020-11-12 12:00:09",
                          "versions": []
                        }
                      ],
                      "_metadata": {
